{
	"name": "foxy",
	"version": "1.0.2",
	"description": "Um Assistente IA",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"test": "tsx ./src/index.ts",
		"build": "tsup && tsc-alias",
		"build:win": "bun build --compile --target=bun-windows-x64 ./src/index.ts --outfile ./binaries/foxy.exe",
		"build:linux": "bun build --compile --target=bun-linux-x64 ./src/index.ts --outfile ./binaries/foxy",
		"build:mac": "bun build --compile --target=bun-macos-x64 ./src/index.ts --outfile ./binaries/foxy",
		"build:mac:arm64": "bun build --compile --target=bun-macos-arm64 ./src/index.ts --outfile ./binaries/foxy"
	},
	"author": "Noa",
	"bin": {
		"foxy": "./dist/index.js"
	},
	"dependencies": {
		"@google/genai": "^0.9.0",
		"axios": "^1.9.0",
		"battery-level": "^3.0.0",
		"chalk": "^5.4.1",
		"commander": "^13.1.0",
		"inquirer": "^12.5.2",
		"is-online": "^11.0.0",
		"zod": "^3.24.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/battery-level": "^3.0.2",
		"@types/node": "^22.14.1",
		"pkg": "^5.8.1",
		"tsc-alias": "^1.8.15",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typedoc": "^0.28.3",
		"typescript": "^5.8.3",
		"vitest": "^3.1.2"
	},
	"license": "ISC"
}
